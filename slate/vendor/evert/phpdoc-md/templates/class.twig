<div class="section class">
    <h{{ level }} id="{{ id }}" class="class-heading">
        {{ shortClass }}
    </h{{ level }}>
    {# namespace #}
    <div class="class-namespace">
        <small>{{ className }}</small>
    </div>
    {# description #}
    <div class="description-heading">
        Description
    </div>
    <p class="class-description">
        {{ description }}

        {{ longDescription|raw }}
    </p>

    {% if isClass %}

        {% if deprecated %}
            <aside class="warning class-deprecated">
                <b>This class is deprecated.</b>
                <br>
                This means that this class will likely be removed in a future version.
            </aside>
        {% endif %}

        <ul>
            {% if abstract %}
                <li>
                    This is an <b>abstract</b> class
                </li>
            {% endif %}
            {% if extends[0] %}
                <li>
                    Parent class: {{ extends.0|classLink|raw }}
                </li>
            {% endif %}
            {% if implements %}
                <li>
                    This class implements: {% for interface in implements %}
                        <b>{{ interface|classLink|raw }}</b>{% if not loop.last %}, {% endif %}{% endfor %}
                </li>
            {% endif %}
        </ul>
    {% endif %}

    {% if isInterface %}

        {% if deprecated %}
            <aside class="warning interface-deprecated">
                <b>This interface is deprecated.</b>
                <br>
                This means that this class will likely be removed in a future version.
            </aside>
        {% endif %}

        <ul>
            <li>
                This is an <b>interfacev</b>
            </li>
            <li>
                Interface name: <b>{{ shortClass }}</b>
            </li>
            {% if extends %}
                <li>
                    *This interface extends: {% for interface in extends %}
                        <b>{{ interface|classLink }}</b>{% if not loop.last %}, {% endif %}{% endfor %}
                </li>
            {% endif %}
        </ul>


    {% endif %}

    {#{% if constants %}#}
    {#Constants#}
    {#----------#}
    {#{% endif %}#}

    {#{% for constant in constants %}#}

    {##### {{ constant.name }}#}

    {#{{ constant.signature|raw }}#}

    {#{{ property.description|raw }}#}

    {#{% if constant.deprecated %}* **Warning:** this constant is **deprecated**. This means that this constant will likely be removed in a future version.#}
    {#{% endif %}#}

    {#{% endfor %}#}

    {#{% if properties %}#}
    {#### Properties#}
    {#{% endif %}#}

    {#{% for property in properties %}#}

    {#<div class="property-heading">#}
    {#{{ property.visibility }}#}
    {#<h4 class="ib">#}
    {#{{ property.name }}#}
    {#</h4>#}
    {#</div>#}

    {#{% if property.deprecated %}#}
    {#<aside class="warning">#}
    {#This property is <b>deprecated</b>.<br>#}
    {#<small>#}
    {#This means that this property will likely be removed in a future version.#}
    {#</small>#}
    {#</aside>#}
    {#{% endif %}#}

    {#{{ property.signature|raw }}#}

    {#{{ property.description|raw }}#}

    {#* Visibility: ****#}

    {#{% if property.static %}* This property is **static**.#}
    {#{% endif %}#}
    {#{% if property.definedBy != className %}* This property is defined by {{ property.definedBy|classLink }}#}
    {#{% endif %}#}

    {#{% endfor %}#}

    {% if methods %}
        <div class="methods">
            <h{{ level + 1 }} id="methods" class="methods-heading">
                Methods
            </h{{ level + 1 }}>


            {% for method in methods %}
                <div class="method">
                    <h{{ level + 2 }} id="{{ method.name|lower }}" class="sup-heading">
                        {{ method.name }}
                    </h{{ level + 2 }}>

                    <div class="method-signature">
                        {{ method.signature|raw }}
                    </div>

                    {% if method.deprecated %}
                        <aside class="warning method-deprecated">
                            <b>This method is deprecated.</b>
                            <br>
                            This means that this class will likely be removed in a future version.
                        </aside>
                    {% endif %}

                    <ul>
                        <li>
                            Visibility: <b>{{ method.visibility }}</b>
                        </li>
                        {% if method.abstract %}
                            <li>
                                This method is <b>abstract</b>.
                            </li>
                        {% endif %}
                        {% if method.static %}
                            <li>
                                This method is <b>static</b>.
                            </li>
                        {% endif %}
                        {% if method.definedBy != className %}
                            <li>
                                This method is defined by: <b>{{ method.definedBy|classLink }}</b>
                            </li>
                        {% endif %}
                    </ul>

                    {% if method.description != "" %}
                        <div class="method-description">
                            <h{{ level + 3 }} id="{{ method.name|lower }}-description" class="sub-heading">
                                Description
                            </h{{ level + 3 }}>
                        </div>
                        <div class="sub-description">
                            {{ method.description|raw }}
                        </div>
                    {% endif %}


                    {% if method.arguments %}
                        <div class="method-arguments">
                            <h{{ level + 3 }} id="{{ method.name|lower }}-arguments" class="sub-heading">
                                Arguments
                            </h{{ level + 3 }}>
                        </div>
                        <ul>
                            {% for argument in method.arguments %}

                                <li>
                                    {% if argument.type %}{{ argument.type|classLink|raw }}{% else %}mixed{% endif %}
                                    <b>{{ argument.name }}</b>
                                    {% if argument.description %}
                                        <p>
                                            {{ argument.description }}
                                        </p>
                                    {% endif %}
                                </li>


                            {% endfor %}
                        </ul>

                    {% endif %}
                </div>
            {% endfor %}
        </div>
    {% endif %}
</div>